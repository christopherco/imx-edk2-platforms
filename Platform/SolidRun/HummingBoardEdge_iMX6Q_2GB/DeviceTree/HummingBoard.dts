					clock-names = "ipg", "per";
					dmas = <0x10 0x19 0x4 0x0 0x10 0x1a 0x4 0x0>;
					dma-names = "rx", "tx";
					status = "okay";
					pinctrl-names = "default";
					pinctrl-0 = <0x12>;
				};

				esai@02024000 {
