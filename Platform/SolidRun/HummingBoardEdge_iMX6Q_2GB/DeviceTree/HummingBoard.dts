					dmas = <0x10 0x3 0x7 0x1 0x10 0x4 0x7 0x2>;
					dma-names = "rx", "tx";
					status = "disabled";
				};

				ecspi@0200c000 {
					#address-cells = <0x1>;
					#size-cells = <0x0>;
					compatible = "fsl,imx6q-ecspi", "fsl,imx51-ecspi";
					reg = <0x200c000 0x4000>;
					interrupts = <0x0 0x20 0x4>;
					clocks = <0x2 0x71 0x2 0x71>;
					clock-names = "ipg", "per";
					dmas = <0x10 0x5 0x7 0x1 0x10 0x6 0x7 0x2>;
